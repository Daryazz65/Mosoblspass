<Page x:Class="Mosoblspass.View.Dispatcher.Pages.DocumentImagePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Mosoblspass.View.Dispatcher.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="970" d:DesignWidth="1530"
      Title="DocumentImagePage">
    <StackPanel Margin="30">
        <TextBlock x:Name="TitleTextBlock" FontSize="20" FontWeight="Bold" Margin="0 0 0 10"/>
        <Image x:Name="DocumentImage" Stretch="Uniform"  Height="500" />
    </StackPanel>
</Page>