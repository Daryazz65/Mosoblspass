<Page x:Class="Mosoblspass.View.Dispatcher.Pages.JournalPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Mosoblspass.View.Dispatcher.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="970" d:DesignWidth="1530"
      Title="JournalPage">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="История поиска" FontSize="20" FontWeight="Bold" Margin="0,0,0,10"/>
            <ListBox Name="HistoryListBox" Height="800" SelectionChanged="HistoryListBox_SelectionChanged"/>
        </StackPanel>
    </Grid>
</Page>